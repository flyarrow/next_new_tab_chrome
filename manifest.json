{
  "manifest_version": 3,
  "name": "标签页整理器",
  "version": "1.0",
  "description": "自动将新打开的标签页排在当前标签页的后面",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "48": "icon.png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}